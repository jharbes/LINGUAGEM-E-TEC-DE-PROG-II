package com.jharbes.layersPractice.entities;

import jakarta.persistence.Entity;

@Entity
public class User {
	
	private Long id;
	private String name;
	private String password;
	
	public User() {
	}

	public User(Long id, String name, String password) {
		super();
		this.id = id;
		this.name = name;
		this.password = password;
	}
	
	
}
